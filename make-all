#!/bin/sh

set -e

for i in \
	aarch64-linux-musl \
	armv5l-linux-musleabi \
	armv5b-linux-musleabi \
	i486-linux-musl \
	x86_64-linux-musl \
	powerpc-linux-muslsf \
	powerpc64le-linux-musl \
	mips-linux-muslsf \
	mipsel-linux-muslsf \
	mips64-linux-muslsf \
	; do
	echo Building target $i
	make TARGET=$i;
done

